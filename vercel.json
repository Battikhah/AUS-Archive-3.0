{
	"version": 2,
	"builds": [
		{
			"src": "app.py",
			"use": "@vercel/python",
			"config": {
				"includeFiles": ["static/**", "templates/**", "Names/**"]
			}
		}
	],
	"routes": [
		{
			"src": "/static/(.*)",
			"dest": "/static/$1",
			"headers": {
				"Cache-Control": "public, max-age=31536000"
			}
		},
		{
			"src": "/(.*)",
			"dest": "app.py"
		}
	],
	"env": {
		"PYTHONUNBUFFERED": "true"
	}
}
